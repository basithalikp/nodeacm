<!-- Team Card Component (for home page horizontal scroll) -->
<div class="card team-card">
    <img src="<%= member.image %>" alt="<%= member.name %>" class="card-img-top" loading="lazy">
    <div class="card-body">
        <h3 class="card-title-team"><%= member.name %></h3>
        <p class="card-text-role"><%= member.role %></p>
    </div>
</div>
